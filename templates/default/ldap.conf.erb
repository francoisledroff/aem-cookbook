com.day.crx {
   // This file was generated by Chef for <%= node['fqdn'] %>.
   // Do NOT modify this file by hand!
   com.day.crx.core.CRXLoginModule sufficient
   com.day.crx.security.ldap.LDAPLoginModule required
   principal_provider.class="com.day.crx.security.ldap.principals.LDAPPrincipalProvider"
   tokenExpiration="<%= node['aem']['ldap']['tokenExpiration']%>"
   host="<%= node['aem']['ldap']['host']%>"
   port="<%= node['aem']['ldap']['port']%>"
   secure="<%= node['aem']['ldap']['secure']%>"
   authDn="<%= @authDn %>"
   authPw="<%= @authPw %>"
   userRoot="<%= @userRoot %>"
   userFilter="<%= @userFilter %>"
   groupRoot="<%= @groupRoot %>"
   groupFilter="(objectClass=Group)"
   userIdAttribute="sAMAccountName"
   groupNameAttribute="name"
   groupMembershipAttribute="uniqueMember"
   autocreate="create"
   autocreate.user.mail="rep:e-mail"
   autocreate.user.cn="rep:fullname"
   autocreate.user.sn="cq:last-name"
   autocreate.user.givenName="cq:first-name"
   autocreate.user.membership="contributor"
   autocreate.user.givenname="profile/givenName"
   autocreate.user.description="profile/title"
   autocreate.group.description="description"
   autocreate.group.cn="rep:fullname"
   autocreate.group.member="member"
   autocreate.path="direct"
   cache.expiration="<%= node['aem']['ldap']['cache.expiration']%>"
   cache.maxsize="<%= node['aem']['ldap']['cache.maxsize']%>";
};
